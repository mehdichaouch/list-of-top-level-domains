{
  "branchPrefix": "renovate-",
  "labels": [
    "dependencies"
  ],
  "packageRules": [
    {
      "enabled": false,
      "matchPackageNames": [
        "*"
      ]
    },
    {
      "groupName": "GitHub Actions",
      "enabled": true,
      "matchPackageNames": [
        "/^actions//"
      ]
    }
  ],
  "commitBodyTable": true,
  "assignees": [
    "@mehdichaouch"
  ],
  "extends": [
    "config:recommended",
    ":gitSignOff",
    "default:disablePrControls"
  ]
}
